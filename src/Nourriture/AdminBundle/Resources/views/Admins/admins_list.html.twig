{% extends '::adminhomebase.html.twig' %}


{% block admin_content %}

<div class="dashboard_container">

	<div class="dashboard_left">
	


    <div class="fieldset">	
		<div class="legend">Recently Registered Users</div>
		<table cellspacing="0" cellpadding="0" border="0" class="admin">
			<thead>
				<tr>
			        <th width="20" align="center">Active</th>
			        <th>Email</th>
			        <th>Signed up</th>
			        <th>Last sign in</th>
			        <th width="200">Actions</th>
				</tr>
			</thead>
			<tbody>
			{% for admin in admins %}
				<tr>
			        <td align="center">
					<img alt="status" src="/bundles/admin/images/{{ admin.enabled ? 'green-dot.png' : 'gray-dot.png' }}">
				</td>
					<td>{{ admin.email }}</td>
					<td>{{ admin.lastlogin | date("Y-m-d H:i:s") }}</td>
					<td>2011-12-14 13:55:07</td>
					<td>
			           	<a class="button edit" href="/admin/admins/edit/{{admin.id}}"><span><span>Edit</span></span></a>
						<a class="button delete" href="/admin/admins/delete/{{admin.id}}"><span><span>Delete</span></span></a>
					</td> 
			    </tr>
			{% endfor %}
			</tbody>
		</table>
	</div>







	</div>
	<div class="dashboard_right">
		<div class="fieldset">
			<div class="legend">Quick Links</div>
			<ul class="large">
			{{ knp_menu_render('admin_admins', {'template':'AdminBundle:Menu:knp_menu.html.twig'}) }}

			</ul>
		</div>
	</div>

</div>


{% endblock %}
